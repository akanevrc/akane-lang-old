ty i64
fn one = 1;

ty i64 -> i64
fn addOne a = a + 1;

ty i64 -> i64 -> i64
fn addNums a b = a + b;

ty i64 -> i64 -> i64
fn doubleAddNums a b = addNums (addNums a b) (addNums a b);

ty i64 -> i64 -> i64
fn addNumsParen a b = (addNums a) b;

ty (i64 -> i64) -> i64 -> i64
fn evalFn1 f a = f a;

ty i64 -> i64
fn evalAddOne a = evalFn1 addOne a;

ty i64 -> i64 -> i64
fn evalAddNums a b = evalFn1 (addNums a) b;

ty i64 -> i64 -> i64 -> i64 -> i64
fn mulAndAdd a b c d = a * b + c * d;

ty i64 -> i64 -> i64
fn mulAndAddOne a b = addOne <| a * b;
